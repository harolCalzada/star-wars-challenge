{
  "name": "star-wars-challenge",
  "version": "1.0.0",
  "description": "Star Wars API Integration with Fastify and Serverless",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "fastify",
    "serverless",
    "typescript",
    "star-wars",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.788.0",
    "@aws-sdk/lib-dynamodb": "^3.788.0",
    "@fastify/aws-lambda": "^5.1.4",
    "@fastify/caching": "^9.0.3",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@sinclair/typebox": "^0.34.33",
    "axios": "^1.8.4",
    "fastify": "^5.3.0",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.6.1",
    "node-cache": "^5.1.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "serverless-dynamodb-local": "^0.1.1",
    "serverless-offline": "^14.4.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
